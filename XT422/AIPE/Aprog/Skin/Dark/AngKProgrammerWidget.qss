QWidget#bgWidget
{
    border-radius: 12px;
    background: transparent;
    border: 2px solid rgba(50, 50, 50, 1);
}

QCheckBox#expandCheckBox::indicator::unchecked
{
    image: url(:/Skin/Dark/CheckBox/logCheckSlectNormalDown.svg);
}

QCheckBox#expandCheckBox::indicator::checked
{
    image: url(:/Skin/Dark/CheckBox/logCheckSelect.svg);
}

QCheckBox#programerCheckBox
{
    color: rgba(227, 227, 227, 1);
    text-align: center;
    font:24px "HarmonyOS Sans SC";
    font-weight: 400;
    text-align: center;
    vertical-align: middle;
}

QCheckBox#programerCheckBox::indicator::unchecked
{
    image: url(:/Skin/Dark/CheckBox/rectCheckHomeNormal.svg);
}

QCheckBox#programerCheckBox::indicator::hover
{
    image: url(:/Skin/Dark/CheckBox/rectCheckHomeHover.svg);
}

QCheckBox#programerCheckBox::indicator::checked
{
    image: url(:/Skin/Dark/CheckBox/rectCheckSelect.svg);
}

QLabel#nameText,
QLabel#nameLabel
{
    color: rgba(227, 227, 227, 1);
    text-align: left;
    vertical-align: middle;
    font:20px "HarmonyOS Sans SC";
    font-weight: 500;
}

QLabel#barValue
{
    color: rgba(227, 227, 227, 1);
    text-align: left;
    vertical-align: middle;
    font: 25px "HarmonyOS Sans SC";
    font-weight: 500;
}

QProgressBar#progressBar::chunk
{
    border-radius: 4px;
    background-color: qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:0,stop:0 rgba(193, 216, 227, 1),stop:1 rgba(39, 205, 253, 1));
}

QProgressBar#progressBar
{
    border-radius: 4px;
    background-color: rgba(40, 40, 40, 1);
}
