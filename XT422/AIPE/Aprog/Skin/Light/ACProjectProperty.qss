QWidget#bgWidget
{
    border-radius: 8px;
    background: rgba(255, 255, 255, 1);
}

QWidget#backgroundWidget
{
    border-radius: 8px;
    background: rgba(247, 247, 247, 1);
}

QPushButton#closeButton
{
    image: url(:/Skin/Light/Common/close.svg);
}

QLabel
{
    color: rgba(95, 99, 104, 1);
    text-align: left;
    vertical-align: middle;
    background: transparent;
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
}

QLabel#Title
{
    font:20px "HarmonyOS Sans SC";
    font-weight: 400;
}

QLineEdit
{
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
    border-radius: 3px;
    padding: 0 8px;
    color: rgba(47, 49, 51, 1);
    background: rgba(235, 235, 235, 1);
}

QLineEdit:read-only
{
    border: 1px solid rgba(95, 99, 104, 1);
    border-radius: 3px;
    padding: 0 8px;
    background: transparent;
}

QPushButton#preButton,
QPushButton#nextButton
{
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    border-radius: 3px;
    background-color: rgba(28, 92, 255, 1);
}

QPushButton#preButton:hover,
QPushButton#nextButton:hover
{
    background-color: rgba(28, 105, 255, 1);
}

QPushButton#preButton:pressed,
QPushButton#nextButton:pressed
{
    background-color: rgba(41, 95, 230, 1);
}

QPushButton#preButton:disabled,
QPushButton#nextButton:disabled
{
	color: rgba(230, 230, 230, 1);
    background-color: rgba(30, 144, 255, 1);
}

QPushButton#renameBtn,
QPushButton#importButton,
QPushButton#saveButton
{
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
    color: rgba(95, 99, 104, 1);
    border-radius: 3px;
    background-color: rgba(255, 255, 255, 1);
    border: 1px solid rgba(247, 247, 247, 1);
}

QPushButton#renameBtn:hover,
QPushButton#importButton:hover,
QPushButton#saveButton:hover
{
    background-color: rgba(250, 250, 250, 1);
}

QPushButton#renameBtn:pressed,
QPushButton#importButton:pressed,
QPushButton#saveButton:pressed
{
    background-color: rgba(235, 235, 235, 1);
}

QPushButton#selectButton
{
    color: rgba(95, 99, 104, 1);
    font:16px "HarmonyOS Sans SC";
    font-weight: 500;
    text-align: center;
    vertical-align: middle;
    border-radius: 7px;
    background-color: rgba(255, 255, 255, 1);
    border: 1px solid rgba(255, 255, 255, 1);
}

QPushButton#selectButton:hover
{
    background-color: rgba(250, 250, 250, 1);
}

QPushButton#selectButton:pressed
{
    background-color: rgba(235, 235, 235, 1);
}

QPushButton#SNCFilePathButton
{
    image:url(:/Skin/Light/PushButton/otherButton.png);
}

QPushButton#addressBtn
{
    image:url(:/Skin/Light/PushButton/otherButton.png);
}

QPushButton#driverButton
{
    color: rgba(95, 99, 104, 1);
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
}

QCheckBox#selectAllCheck,
QCheckBox#checkID,
QCheckBox#pinCheck,
QCheckBox#chipOverLapCheck,
QCheckBox#masterCheck,
QCheckBox#editEnableCheck,
QCheckBox#showPwdCheck,
QCheckBox#UIDSwap
{
    color: rgba(95, 99, 104, 1);
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
}

QCheckBox#clBufferCheck
{
    color: rgba(95, 99, 104, 1);
    font:11px "HarmonyOS Sans SC";
    font-weight: 500;
}

QCheckBox#selectAllCheck::indicator::unchecked,
QCheckBox#clBufferCheck::indicator::unchecked,
QCheckBox#checkID::indicator::unchecked,
QCheckBox#pinCheck::indicator::unchecked,
QCheckBox#chipOverLapCheck::indicator::unchecked,
QCheckBox#masterCheck::indicator::unchecked,
QCheckBox#editEnableCheck::indicator::unchecked,
QCheckBox#showPwdCheck::indicator::unchecked,
QCheckBox#UIDSwap::indicator::unchecked
{
    width:13px;
    height:13px;
    image: url(:/Skin/Light/CheckBox/rectCheckNormalWhite.svg);
}

QCheckBox#selectAllCheck::indicator::hover,
QCheckBox#clBufferCheck::indicator::hover,
QCheckBox#checkID::indicator::hover,
QCheckBox#pinCheck::indicator::hover,
QCheckBox#chipOverLapCheck::indicator::hover,
QCheckBox#masterCheck::indicator::hover,
QCheckBox#editEnableCheck::indicator::hover,
QCheckBox#showPwdCheck::indicator::hover,
QCheckBox#UIDSwap::indicator::hover
{
    width:13px;
    height:13px;
    image: url(:/Skin/Light/CheckBox/rectCheckHoverBlue.svg);
}

QCheckBox#selectAllCheck::indicator::checked,
QCheckBox#clBufferCheck::indicator::checked,
QCheckBox#checkID::indicator::checked,
QCheckBox#pinCheck::indicator::checked,
QCheckBox#chipOverLapCheck::indicator::checked,
QCheckBox#masterCheck::indicator::checked,
QCheckBox#editEnableCheck::indicator::checked,
QCheckBox#showPwdCheck::indicator::checked,
QCheckBox#UIDSwap::indicator::checked
{
    width:13px;
    height:13px;
    image: url(:/Skin/Light/CheckBox/rectCheckSelect.svg);
}

QCheckBox#selectAllCheck:!enabled,
QCheckBox#clBufferCheck:!enabled,
QCheckBox#checkID:!enabled,
QCheckBox#pinCheck:!enabled,
QCheckBox#UIDSwap:!enabled
{
	color: rgb(160, 160, 160);
}

QComboBox
{
    font:15px "HarmonyOS Sans SC Light";
    border-radius: 4px;
    border-top-left-radius: 4x;
    border-bottom-left-radius: 4px;
    background: rgba(235, 235, 235, 1);
}

QComboBox::drop-down
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-radius: 4px;
    background : rgba(235, 235, 235, 1);
}

QComboBox::down-arrow
{
    width: 10px;
    height: 10px;
    image: url(:/Skin/Light/ComboBox/downarrow.svg);
}

QComboBox::QAbstractItemView
{
    background:rgba(255,255,255,1);
    border:1px solid rgba(228,228,228,1);
    border-radius:0px 0px 5px 5px;
    color: rgba(0, 0, 0, 1);
    outline: 0px;
}

QComboBox QAbstractItemView::item
{
  	height:27px;
    font:15px "HarmonyOS Sans SC Light";
  	background: rgba(0, 0, 0, 1);
}

QComboBox QAbstractItemView::item:hover
{
    background:#409CE1;
    color:#ffffff;
}

QComboBox QAbstractItemView::item:selected
{
    background:#409CE1;
    color:#ffffff;
}

QGroupBox
{
    margin-top: 2.3ex;
    border-radius: 10px;
    border: 2px solid rgba(232, 232, 232, 1);
    font:14px "HarmonyOS Sans SC";
    font-weight: 700;
    color: rgba(47, 49, 51, 1);
}

QGroupBox::title
{
    left:10px;
    subcontrol-origin: margin;
    subcontrol-position: top left;
}

QTabWidget::pane
{
    border:none;
    background-color: rgba(247, 247, 247, 1);
}

QTabBar::tab
{
    font:14px "HarmonyOS Sans SC";
    background-color: rgba(235, 235, 235, 1);
}

QTabBar::tab:selected
{
    background-color: rgba(28, 151, 234, 1);
}

QTabBar::tab:hover
{
    background-color: rgba(135, 206, 235, 1);
}