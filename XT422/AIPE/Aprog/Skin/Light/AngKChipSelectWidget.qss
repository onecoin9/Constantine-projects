QWidget#bgWidget
{
    border-radius: 8px;
    background: rgba(247, 247, 247, 1);
}

QLabel
{
    color: rgba(47, 49, 51, 1);
    vertical-align: middle;
    background: transparent;
}

QLabel#searchLabel,
QLabel#adapterLabel,
QLabel#typeLabel,
QLabel#manufactureLabel,
QLabel#totalLabel,
QLabel#numText

{
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
    color: rgba(95, 99, 104, 1);
}

QLabel#deviceConnectText
{
    color: rgba(28, 92, 255, 1);
    text-align: left;
    vertical-align: top;
    background: transparent;
    font:16px "HarmonyOS Sans SC";
    font-weight: 400;
}

QLineEdit#searchLabelEdit
{
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
    border-radius: 3px;
    padding: 0 8px;
    background: rgba(235, 235, 235, 1);
}

/*‰∏ãÊãâÊ°ÜÊ†∑Âº?*/
QComboBox
{
    font:16px "HarmonyOS Sans SC Light";
    border-radius: 4px;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    background: rgba(235, 235, 235, 1);
}

QComboBox::drop-down
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-radius: 4px;
    background : rgba(235, 235, 235, 1);
}

QComboBox::down-arrow
{
    width: 10px;
    height: 10px;
    image: url(:/Skin/Light/ComboBox/downarrow.svg);
}

QComboBox::QAbstractItemView
{
    background:rgba(255,255,255,1);
    border:1px solid rgba(228,228,228,1);
    border-radius:0px 0px 5px 5px;
    color: rgba(37, 37, 37, 1);
    outline: 0px;
}

QComboBox QAbstractItemView::item
{
  	height:27px;
    font:16px "HarmonyOS Sans SC Light";
    background: rgba(255, 255, 255, 1);
    color:rgba(37, 37, 37, 1);
}

QComboBox QAbstractItemView::item:hover
{
    background:#409CE1;
    color:#ffffff;
}

QComboBox QAbstractItemView::item:selected
{
    background:#409CE1;
    color:#ffffff;
}

QPushButton#cancelPushButton,
QPushButton#exportButton
{
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
    color: rgba(95, 99, 104, 1);
    border-radius: 3px;
    background-color: rgba(255, 255, 255, 1);
    border: 1px solid rgba(255, 255, 255, 1);
}

QPushButton#selectPushButton
{
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    border-radius: 3px;
    background-color: rgba(28, 92, 255, 1);
}

QPushButton#selectPushButton:hover
{
    background-color: rgba(28, 105, 255, 1);
}

QPushButton#cancelPushButton:hover,
QPushButton#exportButton:hover
{
    background-color: rgba(250, 250, 250, 1);
}

QPushButton#selectPushButton:pressed
{
    background-color: rgba(41, 95, 230, 1);
}

QPushButton#cancelPushButton:pressed,
QPushButton#exportButton:pressed
{
    background-color: rgba(235, 235, 235, 1);
}

QListView
{
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
    outline:none; /*ËÆæÁΩÆÈÄâ‰∏≠ÂçïÂÖÉÊ†º‰∏çÊòæÁ§∫ËôöÁ∫øÊ°?*/
    border:0px solid #FFFFFF;
    background: rgba(255, 255, 255, 1);
    border: 1px solid #ccc;
    border-radius: 3px;
    padding: 1px;
}

QListView::item
{
    font:18px "HarmonyOS Sans SC";
    font-weight: 700;
    color: rgba(95, 99, 104, 1);
}

QListView::item:hover
{
    background:#b3d9ff;
}

QListView::item:selected
{
    background: #91C8FF;
}

QRadioButton
{
    color: rgba(95, 99, 104, 1);
    font:14px "HarmonyOS Sans SC";
    font-weight: 500;
}
