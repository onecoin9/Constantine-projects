QWidget#widget
{
    background:#ffffff;
}

QToolButton
{
    font:14px "HarmonyOS Sans SC";
    font-weight: 430;
    text-align: center;
    border: none;
    background-color: #ffffff;
    padding-top: 16px;
    padding-bottom: 10px;
}

QToolButton#defineToolButton
{
    qproperty-icon:url(:/Skin/Light/PushButton/defineButtonNormal.svg) center;
}

QToolButton:checked
{
    background-color: rgba(250, 250, 250, 1);
}

